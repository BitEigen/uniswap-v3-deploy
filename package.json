{
  "name": "@biteigen/uniswap-v3-deploy",
  "version": "1.0.4",
  "description": "Deploy Uniswap V3 smart contracts",
  "bin": "dist/index.js",
  "publishConfig": {
    "access": "restricted"
  },
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha",
    "build": "ncc build index.ts -o dist -m",
    "postbuild": "cat shebang.txt dist/index.js > dist/index.cmd.js && mv dist/index.cmd.js dist/index.js",
    "prestart": "yarn build",
    "start": "node dist/index.js",
    "prepublishOnly": "yarn build",
    "postinstall": "typechain --out-dir typechain-types --target ethers-v6 ./node_modules/@uniswap/v3-core/artifacts/contracts/**/*.json ./node_modules/@uniswap/v3-periphery/artifacts/contracts/**/*.json"
  },
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=12.18.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BitEigen/uniswap-v3-deploy.git"
  },
  "author": {
    "email": "contact@biteigen.xyz",
    "name": "BitEigen",
    "url": "https://biteigen.xyz/"
  },
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/BitEigen/uniswap-v3-deploy/issues"
  },
  "homepage": "https://biteigen.xyz/",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@uniswap/sdk-core": "^5.3.1",
    "@uniswap/swap-router-contracts": "1.1.0",
    "@uniswap/v3-core": "1.0.0",
    "@uniswap/v3-periphery": "1.1.1",
    "@uniswap/v3-staker": "1.0.2",
    "hardhat": "^2.22.6",
    "immer": "^7.0.8",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "v3-periphery-1_3_0": "npm:@uniswap/v3-periphery@1.3.0"
  },
  "dependencies": {
    "@uniswap/v3-sdk": "^3.13.1",
    "bignumber.js": "^9.0.1",
    "dotenv": "^16.4.5"
  }
}
